<h1><%= @city.name %></h1>

<form action="/<%=@city.id%>/edit" method="get">
    <input type="hidden" name="name" value="<%=@city.name%>">
    <input type="submit" value="Edit country"/>
</form>

<div class="iframe">
  <p style= 'text-align: center'>
  <iframe width="100%" height="500" src="https://maps.google.com/maps?q=<%=@city.name%>&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
</p>
</div>
